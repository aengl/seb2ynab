{
  "manifest_version": 2,
  "name": "seb2ynab",
  "version": "0.1",
  "background": {
    "persistent": false,
    "scripts": ["background.js"]
  },
  "content_scripts": [{
    "matches": ["*://*.ib.seb.se/*"],
    "js": ["content.js"]
  }],
  "browser_action": {
    "default_title": "SEB YNAB Exporter"
  },
  "permissions": ["activeTab"]
}
